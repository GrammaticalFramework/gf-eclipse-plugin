concrete HelloEng of HelloAbs = {
  flags
    coding = "latin1" ;
  lincat Farewell = {s : Str} ;
  param Gender = Masc | Fem ;
  lincat Greeting = {s : Str} ;
  lincat Recipient = {s : HelloEng.Gender => Str} ;
  oper mega = overload {
                mega : (s : Str) -> {s : HelloEng.Gender => Str;
                                     lock_Recipient : {}}
                     = 0 ;
                mega : (s : Str) -> (r : Str) -> {s : HelloEng.Gender => Str;
                                                  lock_Recipient : {}}
                     = 0 ;
              } ;
  oper superate : Str -> {s : HelloEng.Gender => Str;
                          lock_Recipient : {}} ;
}