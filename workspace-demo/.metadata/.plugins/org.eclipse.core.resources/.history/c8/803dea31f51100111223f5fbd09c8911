--# -path=.:../abstract:../common:../../prelude
resource ResEng = {
	
	param
		Gender = Masc | Fem ;
		
	oper
		superate : Str -> {s : ResEng.Gender => Str} = \s ->
			{ s = \\_ => "super" ++ s } ;
 
		mega2 = overload {
			mega2 : Str -> {s : Gender => Str} = \s ->
				{ s = \\_ => "mega" ++ s } ;
				
			mega2 : Str -> Str -> {s : Gender => Str} = \s,r ->
				{ s = \\_ => "mega" ++ s ++ "and" ++ "mega" ++ r }
		} ;		 
} ;
