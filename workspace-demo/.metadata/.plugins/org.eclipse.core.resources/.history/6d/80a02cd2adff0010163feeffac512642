concrete HelloEng of HelloAbs = ResEng ** {
  flags
    coding = "utf8" ;
  lincat Farewell = {s : Str} ;
  -- ind Fem = canonical ResEng ;
  -- ind Gender = canonical ResEng ;
  lincat Greeting = {s : Str} ;
  -- ind Masc = canonical ResEng ;
  lincat Recipient = {s : ResEng.Gender => Str} ;
  oper mega = overload {
                mega : (s : Str) -> {s : ResEng.Gender => Str; lock_Recipient : {}}
                     = 0 ;
                mega : (s : Str) -> (r : Str) -> {s : ResEng.Gender => Str;
                                                  lock_Recipient : {}}
                     = 0 ;
              } ;
  oper superate : Str -> {s : ResEng.Gender => Str;
                          lock_Recipient : {}} ;
}