concrete NumeralEng of Numeral = CatEng [Numeral,Digits] ** open (ResEng = ResEng) in {
  flags
    coding = "latin1" ;
  lincat Dig = {s : ResEng.CardOrd => ResEng.Case => Str;
                n : ParamX.Number} ;
  lincat Digit = {s : ResEng.DForm => ResEng.CardOrd => ResEng.Case => Str} ;
  -- ind Digits = CatEng ;
  -- ind Numeral = CatEng ;
  lincat Sub10 = {s : ResEng.DForm => ResEng.CardOrd => ResEng.Case => Str;
                  n : ParamX.Number} ;
  lincat Sub100 = {s : ResEng.CardOrd => ResEng.Case => Str;
                   n : ParamX.Number} ;
  lincat Sub1000 = {s : ResEng.CardOrd => ResEng.Case => Str;
                    n : ParamX.Number} ;
  lincat Sub1000000 = {s : ResEng.CardOrd => ResEng.Case => Str;
                       n : ParamX.Number} ;
  oper TDigit : Type ;
  oper commaIf : ParamX.DTail -> Str ;
  oper inc : ParamX.DTail -> ParamX.DTail ;
  oper mk2Dig : Str -> Str -> {s : ResEng.CardOrd => ResEng.Case => Str;
                               n : ParamX.Number} ;
  oper mk3Dig : Str -> Str -> ParamX.Number -> {s : ResEng.CardOrd => ResEng.Case => Str;
                                                n : ParamX.Number} ;
  oper mkDig : Str -> {s : ResEng.CardOrd => ResEng.Case => Str;
                       n : ParamX.Number} ;
}